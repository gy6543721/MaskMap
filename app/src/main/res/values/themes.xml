<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MaskMap" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/material_dynamic_primary10</item>
        <item name="colorPrimaryVariant">@color/design_default_color_primary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/material_dynamic_primary30</item>
        <item name="colorSecondaryVariant">@color/material_dynamic_neutral_variant50</item>
        <item name="colorOnSecondary">@color/material_dynamic_primary99</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>